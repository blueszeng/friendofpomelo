{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "app"
    ],
    "exclude": [],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true
  }
}